          <tr>
            <td><label>Fecha Inicial: </label></td>
            <td>{$FECHAINI}</td>
            <td><label>Fecha Final: </label></td>
            <td>{$FECHAFIN}</td>
            
          </tr>
          <tr>
          </tr>
          <tr>
            <td><label>Tipo Documento: </label></td>
            <td>{$DOCID}</td>
            <td><label>Documento: </label></td>
            <td>{$DOCUMENTO}</td>
          </tr>
          
          
          <tr>
