  {$TABLEGRIDCSS}
  {$TITLETAB}  
  </head>

  <body>
	<fieldset>
        <legend>{$TITLEFORM}</legend>

        <div id="table_find"><table><tr><td><label>Busqueda : </label></td><td>{$BUSQUEDA}</td></tr></table></div>
        
        {$FORM1}
        {$LIQNOMINAID}{$ENCABEZADOID}{$OFICINAID}{$USUARIOID}{$USUARIOID}{$INGRESOFEC}
